<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="html">
    <source path="" />
	<inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />
	<inherits name="com.badlogic.ashley_gwt" />
	<inherits name="com.badlogic.gdx.controllers" />
	<inherits name="com.badlogic.gdx.controllers.controllers-gwt" />
	<inherits name="com.github.tommyettinger.colorful.colorful" />
	<inherits name="com.kotcrab.vis.vis-ui" />
	<inherits name="com.ray3k.tenpatch.tenpatch" />
	<inherits name="formic" />
	<inherits name="guacamole_gdx_gwt" />
	<inherits name="lando.systems.ld52.Main" />
	<inherits name="libgdx_screenmanager" />
	<inherits name="libgdx_screenmanager_gwt" />
	<inherits name="regexodus" />
	<inherits name="simple_graphs" />
	<inherits name="space.earlygrey.shapedrawer" />
	<inherits name="aurelienribon.tweenengine" />
	<inherits name="textratypist" />

    <entry-point class="lando.systems.ld52.gwt.GwtLauncher" />

    <set-configuration-property name="gdx.assetpath" value="../assets" />
    <set-configuration-property name="xsiframe.failIfScriptTag" value="FALSE"/>

	<extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld52.utils.typinglabel.TypingGlyph" />
	<extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld52.utils.typinglabel.effects" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.math.Vector2" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.math.Vector3" />
	<extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld52.utils.Point" />
	<extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld52.particles.Particles" />
	<extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld52.particles.Particle" />


	<!-- These two lines reduce the work GWT has to do during compilation and also shrink output size. -->
    <set-property name="user.agent" value="gecko1_8, safari"/>
    <collapse-property name="user.agent" values="*" />
    <!-- Remove the "user.agent" lines above if you encounter issues with Safari or other Gecko browsers. -->
</module>